{
  "name": "react-ts-eslint",
  "version": "0.0.1",
  "description": "project boilerplate",
  "author": "Yingjie Guo",
  "private": true,
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "serve": "webpack serve --config config/webpack.dev.js --port 8888",
    "start": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run serve\" \"tsc -w\"",
    "build": "webpack --config config/webpack.prod.js --progress",
    "lint": "tsc --noEmit && eslint \"src/*.{js,ts,tsx}\" --quiet --fix",
    "prettier": "prettier -c --write \"src/*.{js,ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "analyze": "env BUNDLE_ANALYZE=true npm webpack --config client/webpack.dev.js",
    "bundle-report": "webpack-bundle-analyzer --port 4200 dist/public/stats.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "antd": "4.16.0",
    "axios": "^0.21.4",
    "buffer": "^6.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^6.2.1",
    "core-js": "^3.17.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "less": "^4.1.1",
    "less-loader": "10.0.1",
    "prettier": "^2.4.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "supertest": "^6.1.6",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "webpack": "^5.52.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0"
  }
}
